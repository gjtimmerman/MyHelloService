<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <system.serviceModel>
    <client>
      <endpoint address="net.tcp://localhost:8081/Hello" binding="netTcpBinding" bindingConfiguration="MyRMBinding" contract="MyHelloService.IHello" name="MyClientConfig"/>
    </client>
    <bindings>
      <customBinding>
        <binding name="MyCustomBinding">
          <binaryMessageEncoding/>
          <httpTransport/>
        </binding>
      </customBinding>
      <netTcpBinding>
        <binding name="MyRMBinding">
          <reliableSession enabled="true" ordered="true"/>
        </binding>
      </netTcpBinding>
    </bindings>
  </system.serviceModel>
</configuration>